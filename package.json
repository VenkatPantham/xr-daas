{
  "name": "xr-daas",
  "version": "1.0.0",
  "description": "XR-DaaS (X-Ray Diagnostics as a Service) is a platform that provides AI-powered diagnostics for chest X-rays. It includes a frontend for user interaction and a backend for data processing and AI integration.",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && python run.py",
    "build": "cd frontend && npm run build",
    "start": "npm run dev",
    "clean": "rimraf node_modules frontend/node_modules backend/node_modules"
  },
  "keywords": [
    "xr",
    "daas",
    "desktop-as-a-service",
    "ai-diagnostics",
    "xray",
    "react",
    "flask",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.0.1",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
